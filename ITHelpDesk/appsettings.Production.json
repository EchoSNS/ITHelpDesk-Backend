{
  "ConnectionStrings": {
    "DefaultConnection": "Server=Change_To_Production_Server_Name;Database=ITHelpDeskDB;User Id=Change_To_UserID;Password=Change_To_Password;TrustServerCertificate=True;Encrypt=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "Change_To_Backend_IPPort",
    "Audience": "Change_To_Frontend_IPPort"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "Change_To_Backend_IPPort"
      }
    }
  },
  "HostSettings": {
    "BackendPort": 4021,
    "FrontendPort": 4000,
    "BackendIp": "Change_To_Backend_IP" // or 0.0.0.0 to listen on all IPs
  },
  "AllowedHosts": "*"
}
